: <br/>\r\n                  <p class=\"font-italic pl-2 mt-5\" >\r\n                  Allow this user to borrow the library resources\r\n                </p>\r\n                </v-card-text>\r\n                <v-card-actions>\r\n                  <v-spacer></v-spacer>\r\n                  <v-btn color=\"grey\" text v-on:click=\"active = false\">\r\n                    Cancel\r\n                  </v-btn>\r\n                  <v-btn color=\"green darken-1\" text v-on:click=\"active = false\">\r\n                    Activate\r\n                  </v-btn>\r\n                </v-card-actions>\r\n              </v-card>\r\n            </v-dialog>\r\n          </span>\r\n        </td>\r\n      </tr>\r\n    </tbody>\r\n  </AdminMgmtResult>\r\n</template>\r\n<script>\r\nimport AdminMgmtResult from \"./base/AdminAccResult\";\r\nexport default {\r\n  name: \"ResultStudent\",\r\n  components: {\r\n    AdminMgmtResult,\r\n  },\r\n  data() {\r\n    return {\r\n      active: false,\r\n      suspend: false\r\n    };\r\n  },\r\n  props: {\r\n    users: {\r\n      type: Array,\r\n    },\r\n  },\r\n};\r\n</script>"]}]}